#include<stdio.h>
int main()
{
  int n,t,r=0;
  printf("Enter the number finding palindrome");
  scanf("%d",&n);
  t=n;
  while(t!=0)
  {
    r=r*10;
    r=r+t%10;
    t=t/10;
  }
  if(n==r)
  {
    printf("The given number %d is a palindrome ",n);
  }
  else
  {
    printf("The given number %d is not a palindrome",n);
  }
}
